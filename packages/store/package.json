{
  "name": "@videojs/store",
  "type": "module",
  "version": "0.1.0-alpha.1",
  "description": "Reactive state management for external systems.",
  "license": "Apache-2.0",
  "main": "dist/default/index.js",
  "module": "dist/default/index.js",
  "types": "dist/dev/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/dev/index.d.ts",
      "development": "./dist/dev/index.js",
      "default": "./dist/default/index.js"
    },
    "./html": {
      "types": "./dist/dev/html.d.ts",
      "development": "./dist/dev/html.js",
      "default": "./dist/default/html.js"
    },
    "./react": {
      "types": "./dist/dev/react.d.ts",
      "development": "./dist/dev/react.js",
      "default": "./dist/default/react.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch ./src --no-clean",
    "dev": "pnpm run build:watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist types"
  },
  "peerDependencies": {
    "@videojs/element": "workspace:*",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@videojs/element": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@videojs/utils": "workspace:*"
  },
  "devDependencies": {
    "@videojs/element": "workspace:*",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.7",
    "jsdom": "^26.1.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "store",
    "media",
    "player",
    "videojs",
    "reactive"
  ]
}
