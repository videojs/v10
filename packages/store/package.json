{
  "name": "@videojs/store",
  "type": "module",
  "version": "0.1.0-alpha.1",
  "description": "Reactive state management for external systems.",
  "license": "Apache-2.0",
  "keywords": [
    "store",
    "media",
    "player",
    "videojs",
    "reactive"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "development": "./dist/dev/index.js",
      "default": "./dist/prod/index.js"
    },
    "./lit": {
      "types": "./dist/types/lit.d.ts",
      "development": "./dist/dev/lit.js",
      "default": "./dist/prod/lit.js"
    },
    "./react": {
      "types": "./dist/types/react.d.ts",
      "development": "./dist/dev/react.js",
      "default": "./dist/prod/react.js"
    }
  },
  "main": "dist/prod/index.js",
  "module": "dist/prod/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch --silent",
    "dev": "pnpm run build:watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist types"
  },
  "peerDependencies": {
    "@lit/context": "^1.1.0",
    "@lit/reactive-element": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@lit/context": {
      "optional": true
    },
    "@lit/reactive-element": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@videojs/utils": "workspace:*"
  },
  "devDependencies": {
    "@lit/context": "^1.1.6",
    "@lit/reactive-element": "^2.1.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.7",
    "jsdom": "^26.1.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
