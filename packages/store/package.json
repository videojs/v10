{
  "name": "@videojs/store",
  "type": "module",
  "version": "0.1.0-preview.10",
  "description": "State management store for Video.js",
  "license": "Apache-2.0",
  "keywords": [
    "store",
    "state",
    "media",
    "player",
    "videojs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch --silent",
    "dev": "pnpm run build:watch",
    "test": "echo \"No tests yet\"",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@videojs/utils": "workspace:*",
    "nanostores": "^1.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "react": "^19.0.0",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
